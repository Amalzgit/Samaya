        .mobile-header-content-area { padding: 15px; }
    }

    /* Product styles */
    .product-cart-wrap {
        position: relative;
        border: 1px solid #ddd;
        border-radius: 8px;
        overflow: hidden;
        margin: 0 auto 20px;
        max-width: 300px;
